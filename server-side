import socket
2
3 dns_table = {"www.google.com": "127.0.1.1",
4 "www.google.com.au": "127.0.1.3",
5 "www.ebay.com.au": "127.0.3.88",
6 "www.youtube.com": "192.178.0.62",
7 "www.amazon.com": "101.022.0.1",
8 "www.deakin.edu.au": "189.162.0.1",
9 "m.youtube.com": "10.0.108.10",
10 "google.com": "110.09.2.1",
11 "youtube.com": "198.0.0.10"}
12
13 svsock = socket.socket(socket.AF_INET, socket.SOCK_DGRAM)
14 print(" Server sequence initiated")
15 print("|| Below will be data obtained ||")
16 svsock.bind(("127.0.2.1", 3388))
17
18 while 2:
19 sv = socket.socket(socket.AF_INET, socket.SOCK_DGRAM)
20 data, address = svsock.recvfrom(3388)
21 print(f"{address} is obtaining data")
22 data = data.decode()
23 ip = dns_table.get(data, " Not Found!").encode()
24 send = svsock.sendto(ip, address)
